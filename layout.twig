<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    {# make sure we always display a proper title: The record's title if there is one, appended with the
       sitename. If there is no title, we append the sitename with the payoff, if there is one. #}
    <title>
        {% if record.title is defined %}{{ record.title|striptags }} | {% endif %}
            {{ app.config.get('general/sitename') }}
        {% if record.title is not defined and app.config.get('general/payoff') %} | {{ app.config.get('general/payoff') }}{% endif %}
    </title>
    <link href="{{ paths.theme }}/css/bootstrap.min.css" rel="stylesheet">
    <link href="{{ paths.theme }}/css/font-awesome.min.css" rel="stylesheet">
    <link href="{{ paths.theme }}/css/prettyPhoto.css" rel="stylesheet">
    <link href="{{ paths.theme }}/css/animate.min.css" rel="stylesheet">
    <link href="{{ paths.theme }}/css/main.css" rel="stylesheet">
    <link href="{{ paths.theme }}/css/responsive.css" rel="stylesheet">
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href="{{ paths.theme }}/images/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ paths.theme }}/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ paths.theme }}/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ paths.theme }}/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="{{ paths.theme }}/images/ico/apple-touch-icon-57-precomposed.png">
</head><!--/head-->

<body class="{{ record.slug }}">
    <header id="header">
        <div class="top-bar">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6 col-xs-4">
                        <div class="top-number"><p><i class="fa fa-phone-square"></i>&nbsp; {{ theme.phone }}</p></div>
                    </div>
                    <div class="col-sm-6 col-xs-8">
                       <div class="social">
                            <ul class="social-share">
                                {% if theme.social.facebook is not empty %}
                                  <li><a href="{{ theme.social.facebook }}" target="_blank"><i class="fa fa-facebook"></i></a></li>
                                {% endif %}
                                {% if theme.social.twitter is not empty %}
                                  <li><a href="{{ theme.social.twitter }}" target="_blank"><i class="fa fa-twitter"></i></a></li>
                                {% endif %}
                                {% if theme.social.linkedin is not empty %}
                                  <li><a href="{{ theme.social.linkedin }}" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                                {% endif %}
                                {% if theme.social.gplus is not empty %}
                                  <li><a href="{{ theme.social.gplus }}" target="_blank"><i class="fa fa-google-plus"></i></a></li>
                                {% endif %}
                                {% if theme.social.skype is not empty %}
                                  <li><a href="{{ theme.social.skype }}" target="_blank"><i class="fa fa-skype"></i></a></li>
                                {% endif %}
                            </ul>
                            <div class="search">
                                <form role="form" method="get" action="{{ paths.root }}search" id="searchform" enctype="text/plain">
                                  <input type="search" class="search-form" value="{% if search is defined %}{{ search|escape }}{% endif %}" placeholder="{{ __("Search â€¦") }}" name="search">
                                    <i class="fa fa-search"></i>
                                </form>
                            </div>
                       </div>
                    </div>
                </div>
            </div><!--/.container-->
        </div><!--/.top-bar-->
        <nav class="navbar navbar-inverse" role="banner">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">{{ __('Toggle mavigation') }}</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="{{ paths.root }}"><img src="{{ paths.theme }}/images/logo.png" alt="logo"></a>
                </div>

                <div class="collapse navbar-collapse navbar-right">
                    <ul class="nav navbar-nav">
                        {# Include the menu, as defined in config/menu.yml. Note that this theme comes with it's own _sub_menu.twig. If this file wasn't included in the current theme, the default menu template in app/theme_defaults/_sub_menu.twig would be used. #}
                        {{ menu('main', '_sub_menu.twig') }}
                    </ul>
                </div>
            </div><!--/.container-->
        </nav><!--/nav-->

    </header>

    {# This is the flexible part #}
     {% block body %}{% endblock %}

    <section id="bottom">
    <div class="container wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="600ms">
        <div class="row">
            <div class="col-md-3 col-sm-6">
                <div class="widget">
                    <h3>Company</h3>
                    <ul>
                        <li><a href="#">About us</a></li>
                        <li><a href="#">We are hiring</a></li>
                        <li><a href="#">Meet the team</a></li>
                        <li><a href="#">Copyright</a></li>
                        <li><a href="#">Terms of use</a></li>
                        <li><a href="#">Privacy policy</a></li>
                        <li><a href="#">Contact us</a></li>
                    </ul>
                </div>
            </div><!--/.col-md-3-->

            <div class="col-md-3 col-sm-6">
                <div class="widget">
                    <h3>Support</h3>
                    <ul>
                        <li><a href="#">Faq</a></li>
                        <li><a href="#">Blog</a></li>
                        <li><a href="#">Forum</a></li>
                        <li><a href="#">Documentation</a></li>
                        <li><a href="#">Refund policy</a></li>
                        <li><a href="#">Ticket system</a></li>
                        <li><a href="#">Billing system</a></li>
                    </ul>
                </div>
            </div><!--/.col-md-3-->

            <div class="col-md-3 col-sm-6">
                <div class="widget">
                    <h3>Developers</h3>
                    <ul>
                        <li><a href="#">Web Development</a></li>
                        <li><a href="#">SEO Marketing</a></li>
                        <li><a href="#">Theme</a></li>
                        <li><a href="#">Development</a></li>
                        <li><a href="#">Email Marketing</a></li>
                        <li><a href="#">Plugin Development</a></li>
                        <li><a href="#">Article Writing</a></li>
                    </ul>
                </div>
            </div><!--/.col-md-3-->

            <div class="col-md-3 col-sm-6">
                <div class="widget">
                    <h3>Our Partners</h3>
                    <ul>
                        <li><a href="#">Adipisicing Elit</a></li>
                        <li><a href="#">Eiusmod</a></li>
                        <li><a href="#">Tempor</a></li>
                        <li><a href="#">Veniam</a></li>
                        <li><a href="#">Exercitation</a></li>
                        <li><a href="#">Ullamco</a></li>
                        <li><a href="#">Laboris</a></li>
                    </ul>
                </div>
            </div><!--/.col-md-3-->
        </div>
    </div>
</section><!--/#bottom-->

<footer id="footer" class="midnight-blue">
    <div class="container">
        <div class="row">
            <div class="col-sm-6">
              Copyright: &copy; {{ now|date("Y") }} <a target="_blank" href="{{ theme.copyright.link }}" title="{{ theme.copyright.title }}"> {{ theme.copyright.name }} </a>. All Rights Reserved.
            </div>
            <div class="col-sm-6">
                <ul class="pull-right">
                    {# Check if a footer menu with items exist before we output it here #}
                    {% if app.config.get('menu/footer') is not empty %}
                        {{ menu('footer', '_sub_menu.twig') }}
                    {% else %}
                        No menu defined
                    {% endif %}
                </ul>
            </div>
        </div>
    </div>
</footer><!--/#footer-->

<script src="{{ paths.theme }}/js/jquery.js"></script>
<script src="{{ paths.theme }}/js/bootstrap.min.js"></script>
<script src="{{ paths.theme }}/js/jquery.prettyPhoto.js"></script>
<script src="{{ paths.theme }}/js/jquery.isotope.min.js"></script>
<script src="{{ paths.theme }}/js/main.js"></script>
<script src="{{ paths.theme }}/js/wow.min.js"></script>
</body>
</html>
